import java.util.*;


class Calcul{
	int a,b;
	Calcul(int new_a, int new_b){
		a=new_a;
		b=new_b;
	}
	
	void bigsmall() {
		if(this.a>this.b) {
			System.out.println(">");
		}
		else if(this.a<this.b) {
			System.out.println("<");
		}
		else
			System.out.println("==");
	}
}

public class Main {

	public static void main(String[] args) {
		int a,b;
		Scanner sc = new Scanner(System.in);
		a=sc.nextInt();
		b=sc.nextInt();
		Calcul ca = new Calcul(a,b);
		ca.bigsmall();
		
	}
}
