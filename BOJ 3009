#include<stdio.h>

int main(void)
{
	int x[3], y[3];
	int a=0,b=0,fx,fy;
	scanf("%d %d", &x[0], &y[0]);
	scanf("%d %d", &x[1], &y[1]);
	if(x[1]!=x[0]){
		a= x[1]+x[0];
	}
	if(y[1]!=y[0]){
		b=y[1]+y[0];
	}
	scanf("%d %d", &x[2], &y[2]);
	if(x[2]!=x[0]){
		a= x[2]+x[0];
	}
	if(y[2]!=y[0]){
		b= y[2]+y[0];
	}
	fx= 2*a-(x[0]+x[1]+x[2]);
	fy= 2*b-(y[0]+y[1]+y[2]);
	printf("%d %d", fx, fy);
	
}
